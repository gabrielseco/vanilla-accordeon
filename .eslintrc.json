{
  "parser": "babel-eslint",  
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": "airbnb-base",
  "rules": {
    "arrow-body-style": "off",
    "class-methods-use-this": "off",
    "linebreak-style": "off",
    "no-param-reassign": "off",
    "no-useless-constructor": "off",
    "prefer-template": "off",
    "import/prefer-default-export": "off"
  }
}